{
  "expo": {
    "name": "TimeManager",
    "slug": "TimeManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "jsEngine": "hermes"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.148201193183-tc11ghq6ecuvlbb4blcslvg6oustjod9"
        }
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.developingprogress.TimeManager",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.developingprogress.TimeManager",
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-community-flipper",
      "@notifee/react-native"
    ]
  }
}
